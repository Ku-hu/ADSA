#include <stdio.h>

int n, graph[20][20], path[20];

int isSafe(int v, int pos) {
    if (!graph[path[pos - 1]][v]) return 0;   // Check adjacency
    for (int i = 0; i < pos; i++)             // Check if already included
        if (path[i] == v) return 0;
    return 1;
}

void hamiltonianCycle(int pos) {
    if (pos == n) {
        if (graph[path[pos - 1]][path[0]]) {  // Complete the cycle
            for (int i = 0; i < n; i++) printf("%d ", path[i]);
            printf("%d\n", path[0]);
        }
        return;
    }

    for (int v = 1; v < n; v++) {
        if (isSafe(v, pos)) {
            path[pos] = v;
            hamiltonianCycle(pos + 1);
        }
    }
}

int main() {
    printf("Number of vertices: ");
    scanf("%d", &n);

    printf("Enter adjacency matrix:\n");
    for (int i = 0; i < n; i++)
        for (int j = 0; j < n; j++)
            scanf("%d", &graph[i][j]);

    path[0] = 0;
    printf("Hamiltonian Cycles:\n");
    hamiltonianCycle(1);

    return 0;
}
